set_a = {1, 2, 3, 4}
set_b = {3, 4, 5, 6}

for a in set_a:
    set_a.add(2,4)
    print(a)


# Union: Elements that are in either set
print(set_a | set_b)  # Output: {1, 2, 3, 4, 5, 6}

# Intersection: Elements that are in both sets
print(set_a & set_b)  # Output: {3, 4}

# Difference: Elements that are in set_a but not in set_b
print(set_a - set_b)  # Output: {1, 2}

# Symmetric Difference: Elements that are in either set, but not both
print(set_a ^ set_b) 